#Android fstab file.
#<src>                                                  <mnt_point>         <type>    <mnt_flags and options>                       <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
#/dev/block/bootdevice/by-name/system    /system    ext4    rw    wait,verify
/dev/block/by-name/modem_secure	/mnt/modem/modem_secure	ext4	rw,noauto_da_alloc,nosuid,nodev,noatime,data=ordered,barrier=1,context=u:object_r:modem_secure_file:s0	wait,check,notrim,nofail
/dev/block/by-name/modem_om    /modem_log    ext4    rw,noauto_da_alloc,nosuid,nodev,noatime,data=ordered,barrier=1,context=u:object_r:modem_log_file:s0    wait,check,notrim,nofail
/dev/block/by-name/modem_fw    /vendor/modem/modem_fw    ext4    ro,context=u:object_r:modem_fw_file:s0 wait,nofail
/dev/block/by-name/modemnvm_img    /mnt/modem/mnvm2:0    ext4   rw,noauto_da_alloc,nosuid,nodev,noatime,data=ordered,barrier=1,context=u:object_r:modem_nv_file:s0   wait,check,formattable,notrim,nofail